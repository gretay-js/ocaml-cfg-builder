(lang dune 1.8)
(using fmt 1.1)
(name ocamlcfg)
(package
 (name ocamlcfg)
 (synopsis "Compiler library for building Control Flow Graph (CFG) for OCaml")
 (description "A library for converting from low-level representation
of OCaml programs, generated by OCaml compiler for native code, to CFG and back.")
 )
