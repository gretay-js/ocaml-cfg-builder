(lang dune 1.10)
(using fmt 1.1)
(name ocamlcfg)
(version 0.1.0)

; (license MIT)
(maintainers "Greta Yorsh <gyorsh@janestreet.com>")
(authors "Greta Yorsh <gyorsh@janestreet.com>")
(source (github gretay-js/ocamlfdo))

(package
 (name ocamlcfg)
 (synopsis "Compiler library for building Control Flow Graph (CFG) for OCaml")
 (description "
A library for converting from low-level representation of OCaml programs,
generated by OCaml compiler for native code, to CFG and back.

Copyright (c) 2019--2020 Jane Street Group LLC.
")


 (depends
  (ocaml (>= 4.08))
  (dune (>= 1.8))
 )
 )

(generate_opam_files true)
